    android:textColor="@color/LIGHTBLUE"
    android:id="@+id/textView"
    android:textSize="30dp"
    android:layout_alignTop="@+id/TextViewCountDown"
    android:layout_centerHorizontal="true"
    android:layout_marginTop="51dp" />

